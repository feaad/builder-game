generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model animals {
  id   String @id @default(auto()) @map("_id") @db.ObjectId
  name String
}

model countries {
  id   String @id @default(auto()) @map("_id") @db.ObjectId
  code String
  name String
}
